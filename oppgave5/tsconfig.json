{
  "compilerOptions": {
    "lib": [
      "es5", "es6", "dom", "es2015.promise"
    ],
    "moduleResolution": "node",
    "noImplicitAny": true,
    "module": "es6",
    "target": "es5",
    "jsx": "react",
    "allowJs": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "outDir": "build/typescript"
  },
  "include": [
    "oppgave5/src/**/*.ts",
    "oppgave5/src/**/*.tsx",
    "oppgave5/lib/**/*.js"
  ],
  "exclude": [
    "oppgave5/node_modules"
  ]
}